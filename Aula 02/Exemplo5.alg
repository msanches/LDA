algoritmo media
	real n1, n2, n3, n4, media
	inicio
		escreva("Digite o primeiro número: ")
		leia(n1)
		escreva("Digite o segundo número: ")
		leia(n2)
		escreva("Digite o terceiro número: ")
		leia(n3)
		escreva("Digite o quarto número: ")
		leia(n4)
		media = (n1+n2+n3+n4)/4
		escreva("A média dos números digitados é: " + media)
	fim 
	
	